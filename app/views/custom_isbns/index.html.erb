<div class="row">
  <div class="col-sm">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3">
      <h1 class="h2">Custom Isbns</h1>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm">
    <div class="btn-toolbar mb-2 mb-md-0 float-end">
      <div class="btn-grp mr-2">
        <%= button_to 'New Custom Isbn', new_custom_isbn_path, :method => "get", class: "btn btn-secondary" %>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm table-responsive">
    <table class="table table-striped text-center" id="custom_isbns-table" data-source="<%= custom_isbns_path(format: :json) %>" data-turbolinks='false'>
      <thead>
        <tr>
          <th>Text isbn</th>
          <th>Alt isbn</th>
          <th>Custom isbn</th>
          <th>Code isbn</th>
          <th>Tag</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
      </tbody>
    </table>
  </div>
</div>