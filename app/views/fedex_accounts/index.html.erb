<div class="row">
  <div class="col-sm">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3">
      <h1 class="h2">Fedex Accounts</h1>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm">
    <div class="btn-toolbar mb-2 mb-md-0 float-end">
      <div class="btn-grp mr-2">
        <%= button_to 'New Fedex Account', new_fedex_account_path, :method => "get", class: "btn btn-secondary" %>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm table-responsive">
    <table class="table table-striped">
      <thead>
          <th>Key</th>
          <th>Password</th>
          <th>Account number</th>
          <th>Meter number</th>
          <th>Company name</th>
          <th>Phone number</th>
          <th>Street</th>
          <th>City</th>
          <th>State</th>
          <th>Zip code</th>
          <th>Prod</th>
          <th>Service Type</th>
          <th colspan="3">Actions</th>
      </thead>
      <tbody>
        <% @fedex_accounts.each do |fedex_account| %>
          <tr>
            <td><%= fedex_account.key %></td>
            <td><%= fedex_account.password %></td>
            <td><%= fedex_account.account_number %></td>
            <td><%= fedex_account.meter_number %></td>
            <td><%= fedex_account.company_name %></td>
            <td><%= fedex_account.phone_number %></td>
            <td><%= fedex_account.street %></td>
            <td><%= fedex_account.city %></td>
            <td><%= fedex_account.state %></td>
            <td><%= fedex_account.zip_code %></td>
            <td><%= fedex_account.prod %></td>
            <td><%= fedex_account.service_type %></td>
            <td colspan="3">
              <%= link_to 'Show', fedex_account %> |
              <%= link_to 'Edit', edit_fedex_account_path(fedex_account) %> |
              <%= link_to 'Destroy', fedex_account, method: :delete, data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>