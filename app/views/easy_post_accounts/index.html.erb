<div class="row">
  <div class="col-sm">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3">
      <h1 class="h2">EasyPost Accounts</h1>
    </div>
  </div>
  <div class="col-sm">

  </div>
</div>
<div class="row">
  <div class="col-sm">
    <div class="btn-toolbar mb-2 mb-md-0 float-end">
      <div class="btn-grp mr-2">
        <%= button_to 'New EasyPost Account', new_easy_post_account_path, :method => "get", class: "btn btn-secondary" %>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm">
    <table class="table table-striped" id="easy_post_accounts-table" data-source="<%= easy_post_accounts_path(format: :json) %>" data-turbolinks='false'>
      <thead>
        <tr>
          <th>Key</th>
          <th>Account number</th>
          <th>Name</th>
          <th>Company name</th>
          <th>Phone number</th>
          <th>Email</th>
          <th>Street</th>
          <th>City</th>
          <th>State</th>
          <th>Zip code</th>
          <th>Parcel Width</th>
          <th>Parcel Length</th>
          <th>Parcel Height</th>
          <th>Parcel Weight</th>
          <th>Prod</th>
          <th></th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>

      </tbody>
    </table>
  </div>
</div>