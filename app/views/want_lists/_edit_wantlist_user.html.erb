<label>Select users</label><br>
<% if @selected_users %>
<%@selected_users.each do |user|%>
    <% unless user.email == @current_user.email %>
        <%=check_box("user_id", user.id, {:checked => true, :multiple => true})%>
        <%= user.email %>
        <br>
    <% end %>
    <% end %>
<% end %>

<% if @remaining_users %>
<%@remaining_users.each do |user|%>
    <% unless user.email == @current_user.email %>
        <%=check_box("user_id", user.id, {:multiple => true})%>
        <%= user.email %>
        <br>
    <% end %>
    <% end %>
<% end %>

